class Solution(object):
    def wordBreak(self, s, wordDict):
        """
        :type s: str
        :type wordDict: List[str]
        :rtype: List[str]
        """
        for w in wordDict:
            print w
            s2 = s.replace(w,w+' ')
            print s2
        return s2    
S = Solution()
s = 'catsanddog'
wordDict = ["cat","cats","and","sand","dog"]
S.wordBreak(s, wordDict)