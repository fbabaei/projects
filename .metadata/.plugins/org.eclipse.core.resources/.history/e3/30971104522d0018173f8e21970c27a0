class Num():
    def __init__(self):
        self.head = None
        class Digit():
            def __init__(self,digit):
                self.digit = digit 
                self.nextDigit = None 
        self.tail = self.Digit()
        

        
    def mkNum(self,n):
        while n > 0:
            self.tail.nextDigit = self.Digit(divmod(n, 10))
            
            n = n/10
       
num1 = Num()
num2 = Num()

def addNums(num1,num2):
    return num1 + num2
N = Num()
print N.mkNum(102)


            