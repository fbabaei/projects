from __builtin__ import next
class Solution(object):
    def wordBreak(self, s, wordDict):
        """
        :type s: str
        :type wordDict: List[str]
        :rtype: List[str]
        """
        temp = s 
        tempDict = wordDict           
        for w in tempDict:
            if w in temp:
                temp = temp.replace(w,'')
                temp.strip()
                out = w+" "
                print out
            else:
                next
        return out    
S = Solution()
s = 'catsanddog'
wordDict = ["cat","cats","and","sand","dog"]
print S.wordBreak(s, wordDict)